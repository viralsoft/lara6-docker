{
    "name": "virals-package/virals-inventory",
    "description": "Inventory Management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ViralS-longnd",
            "email": "longnd@viralsoft.vn"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "barryvdh/laravel-dompdf": "^0.8.0@dev",
        "virals-package/virals-base": "dev-master"
